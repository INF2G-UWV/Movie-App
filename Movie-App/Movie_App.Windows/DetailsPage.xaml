    <!--
        This grid acts as a root panel for the page that defines two rows:
        * Row 0 contains the back button and page title
        * Row 1 contains the rest of the page layout
    -->
	<Grid Width="1920" Height="1080">
		<Grid.Background>
			<ImageBrush Stretch="Fill" ImageSource="Assets/images/background.png" />
		</Grid.Background>
		<Grid.ChildrenTransitions>
			<TransitionCollection>
				<EntranceThemeTransition />
			</TransitionCollection>
		</Grid.ChildrenTransitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="140" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>

		<Grid.DataContext>
			<DataModel:ViewModel />
		</Grid.DataContext>
		<!--
            TODO: Content should be placed within the following grid 
                  to show details for the current item
        -->

		<!-- Back button and page title -->

		<!-- Back button and page title -->

		<!-- Back button and page title -->
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="120" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<SearchBox HorizontalAlignment="Left" FocusOnKeyboardInput="True" VerticalAlignment="Top" Margin="1461,60,0,0" Height="45" Width="329" QuerySubmitted="SearchBox_QuerySubmitted" Grid.Column="1"/>

			<Button x:Name="backButton" Margin="39,59,39,0"
				Command="{Binding NavigationHelper.GoBackCommand, ElementName=pageRoot}"
				Style="{StaticResource NavigationBackButtonNormalStyle}"
				VerticalAlignment="Top"
				AutomationProperties.Name="Back"
				AutomationProperties.AutomationId="BackButton"
				AutomationProperties.ItemType="Navigation Button" />
			<TextBlock x:Name="pageTitle" Style="{StaticResource HeaderTextBlockStyle}" Grid.Column="1"
				IsHitTestVisible="false" TextWrapping="NoWrap" VerticalAlignment="Bottom" Margin="0,0,30,40"
				FontFamily="Letter Gothic Std" Text="{Binding [0].Title}">
				<TextBlock.DataContext>
					<rottenTomatoes:MoviesDataSource />
				</TextBlock.DataContext>
			</TextBlock>
		</Grid>
		<Grid x:Name="LargeImage" HorizontalAlignment="Left" Height="940" VerticalAlignment="Top" Width="640"
			Grid.Row="1">
			<Grid.DataContext>
				<rottenTomatoes:MoviesDataSource />
			</Grid.DataContext>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="70" />
				<ColumnDefinition Width="480" />
				<ColumnDefinition Width="70" />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="100" />
				<RowDefinition Height="725" />
				<RowDefinition Height="50" />
			</Grid.RowDefinitions>
			<Image HorizontalAlignment="Left" Height="705" VerticalAlignment="Top" Width="460" Grid.Column="1"
				Margin="10,10,0,0" Grid.Row="1" Stretch="Fill" Source="{Binding [0].PosterOriginal}">
				<Image.DataContext>
					<rottenTomatoes:MoviesDataSource />
				</Image.DataContext>
			</Image>
			<Button Content="Click here for Cinema times" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="7,0,0,25" Height="53" Width="466" FontFamily="Letter Gothic Std" Click="Button_Click" Grid.Column="1" Foreground="White" FontSize="21.333">
				<Interactivity:Interaction.Behaviors>
					<Core:EventTriggerBehavior EventName="Click">
						<Core:NavigateToPageAction Parameter="{Binding [0].movieId}" TargetPage="Movie_App.InTheathers"/>
					</Core:EventTriggerBehavior>
				</Interactivity:Interaction.Behaviors>
			</Button>
		</Grid>
		<Grid x:Name="Details" HorizontalAlignment="Left" Height="940" VerticalAlignment="Top" Width="640"
			Margin="640,0,0,0" Grid.Row="1">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="210" />
				<ColumnDefinition />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="175" />
				<RowDefinition Height="65" />
				<RowDefinition Height="65" />
				<RowDefinition Height="65" />
				<RowDefinition Height="65" />
				<RowDefinition Height="425" />
			</Grid.RowDefinitions>
			<Grid.DataContext>
				<rottenTomatoes:MoviesDataSource />
			</Grid.DataContext>
			<TextBlock x:Name="Header" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Details:"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="48" Margin="10,101,0,0"
				Height="59" Width="190" />
			<TextBlock x:Name="Title" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top"
				FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Margin="10,10,0,0" Height="35"
				Grid.Row="1" Width="190">
				<Run Text="Title:" />
				<LineBreak />
				<Run />
			</TextBlock>
			<TextBlock x:Name="Score" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top"
				FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Margin="10,10,0,0" Height="35"
				Grid.Row="2" Width="190" Text="Score Rating:" />
			<TextBlock x:Name="Runtime" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top"
				FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Margin="10,10,0,0" Height="35"
				Grid.Row="3" Width="190">
				<Run Text="Runtime:" />
				<LineBreak />
				<Run />
				<LineBreak />
				<Run />
			</TextBlock>
			<TextBlock x:Name="Release" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top"
				FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Height="35" Text="Release Date:"
				Grid.Row="4" Width="190" Margin="10,10,0,0" />
			<TextBlock x:Name="Summary" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top"
				FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Margin="10,10,0,0" Height="35"
				Grid.Row="5" Width="190">
				<Run Text="Summary" />
				<Run Text=":" />
			</TextBlock>
			<TextBlock x:Name="HeaderBinding" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding [0].Title}"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="26.667" Grid.Column="1"
				Margin="10,10,0,0" Grid.Row="1" Width="410" Height="65" Grid.RowSpan="2" />
			<TextBlock x:Name="ScoreBinding" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding [0].RatingsAudience}"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="26.667" Grid.Column="1"
				Margin="10,10,0,0" Grid.Row="2" Width="410" Height="45" />
			<TextBlock x:Name="RuntimeBinding" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding [0].Runtime}"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="26.667" Grid.Column="1"
				Margin="10,10,0,0" Grid.Row="3" Width="410" Height="45" />
			<TextBlock x:Name="ReleaseBinding" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding [0].ReleaseDatesTheater}"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="26.667" Grid.Column="1"
				Margin="10,10,0,0" Grid.Row="4" Width="410" Height="45" />
			<TextBlock x:Name="SummaryBinding" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding [0].Synopsis}"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="26.667" Margin="10,50,0,-70"
				Grid.Row="5" Width="620" Height="445" Grid.ColumnSpan="2" TextAlignment="Justify" />


		</Grid>
		<Grid x:Name="Actors" HorizontalAlignment="Left" Height="940" VerticalAlignment="Top" Width="640"
			Margin="1362,0,-82,0" Grid.Row="1">
			<Grid.RowDefinitions>
				<RowDefinition Height="175" />
				<RowDefinition />
			</Grid.RowDefinitions>
			<Grid.DataContext>
				<rottenTomatoes:MoviesDataSource />
			</Grid.DataContext>

			<TextBlock x:Name="Header1" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Actors:"
				VerticalAlignment="Top" FontFamily="Century Gothic" FontSize="48" Margin="10,101,0,0"
				Height="59" Width="190" />
			<TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding [0].NameActor}"
				VerticalAlignment="Top" Height="660" Width="620" Margin="10,10,0,0" FontFamily="Century Gothic"
				FontSize="26.667" Grid.Row="1" />
		</Grid>

		<!-- Back button and page title -->
	</Grid>
</Page>